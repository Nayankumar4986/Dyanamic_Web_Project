    <section id="contact_us_section" class="py-5 background1">

      <div class="container">
        <h3 class="text-center text-secondary">Contact Us</h3>
        <div class="row">
          <div class="col-md-12">

            <form action="/process-contact-form" method="post">
              <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Your Email
                  address</label>
                <input name="email" type="email" class="form-control"
                  id="exampleInputEmail1"
                  aria-describedby="emailHelp">
                <div id="emailHelp" class="form-text">We'll never share your
                  email with anyone else.</div>
              </div>

              <div class="mb-3">
                <label for="conttactNumberfield" class="form-label">Your Contact
                  Number</label>
                <input name="phone" type="number" class="form-control"
                  id="conttactNumberfield">
              </div>

              <div class="form-floating mt-2">
                <textarea name="query" style=" height: 250px" ;
                  class="form-control" placeholder="Leave a comment here"
                  id="queryField"></textarea>
                <label for="queryField">Your Query</label>
              </div>

              <div class="container text-center">
                <button type="submit" class="btn btn-dark my-4">Submit</button>
              </div>
            </form>

          </div>
        </div>
